<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:cardCornerRadius="20dp"
    android:paddingBottom="30dp"
    >
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/border">
    <Button
        android:id="@+id/upd"
        android:layout_width="217dp"
        android:layout_height="31dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="184dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:text="edit home details"
        android:textSize="10sp" />

    <Button
        android:id="@+id/delete"
        android:layout_width="217dp"
        android:layout_height="31dp"
        android:layout_marginStart="184dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:layout_below="@+id/upd"
        android:text="Delete home"
        android:textSize="10sp" />




    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/com_name"
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="46dp"

            android:fontFamily="@font/baloon"
            android:text="Community Name"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/com_name2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"

            android:layout_marginRight="20dp"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/add"
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"

            android:fontFamily="@font/baloon"
            android:text="Address"

            android:textColor="@color/black" />

        <TextView
            android:id="@+id/address_flat2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"

            android:layout_marginRight="20dp"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/address_street2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="20dp"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/address_city2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="20dp"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/address_state2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="20dp"
            android:textColor="@color/black" />


        <TextView
            android:id="@+id/pin2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="20dp"
            android:textColor="@color/black" />


        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="359dp">

            <TextView
                android:id="@+id/bed"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="30dp"

                android:fontFamily="@font/baloon"
                android:text="No.Of.Bedrooms"
                android:textColor="@color/black" />

            <TextView
                android:id="@+id/sq_ft"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="28dp"
                android:fontFamily="@font/baloon"
                android:text="Square Feet"
                android:textColor="@color/black" />


            <TextView
                android:id="@+id/rent"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="51dp"
                android:fontFamily="@font/baloon"
                android:text="Rent"

                android:textColor="@color/black" />

            <TextView
                android:id="@+id/advance"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="73dp"
                android:fontFamily="@font/baloon"
                android:text="Advance"

                android:textColor="@color/black" />

            <TextView
                android:id="@+id/bed2"
                android:layout_width="119dp"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="11dp"

                android:layout_toRightOf="@+id/bed"
                android:textColor="@color/black" />

            <TextView
                android:id="@+id/sq_ft2"
                android:layout_width="124dp"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="11dp"
                android:layout_marginTop="29dp"
                android:layout_marginBottom="130dp"
                android:layout_toRightOf="@+id/sq_ft"
                android:textColor="@color/black" />

            <TextView
                android:id="@+id/rent2"
                android:layout_width="124dp"
                android:layout_height="wrap_content"

                android:layout_alignParentTop="true"
                android:layout_marginLeft="11dp"

                android:layout_marginTop="52dp"

                android:layout_toRightOf="@id/rent"
                android:textColor="@color/black" />


            <TextView
                android:id="@+id/advance2"
                android:layout_width="124dp"
                android:layout_height="20dp"

                android:layout_alignParentTop="true"
                android:layout_marginLeft="11dp"
                android:layout_marginTop="74dp"

                android:layout_toRightOf="@+id/advance"
                android:textColor="@color/black" />

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="wrap_content"
                android:layout_height="130dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginStart="31dp"
                android:layout_marginLeft="31dp"
                android:layout_marginTop="96dp"
                android:layout_marginEnd="209dp"

                android:layout_marginRight="209dp" />

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="168dp"
                android:layout_height="125dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginStart="213dp"
                android:layout_marginLeft="213dp"
                android:layout_marginTop="101dp"
                android:layout_marginEnd="30dp"

                android:layout_marginRight="30dp" />

            <ImageView
                android:id="@+id/imageView13"
                android:layout_width="170dp"
                android:layout_height="129dp"
                android:layout_below="@id/imageView11"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_marginStart="31dp"
                android:layout_marginLeft="31dp"
                android:layout_marginTop="7dp" />

            <ImageView
                android:id="@+id/imageView14"
                android:layout_width="175dp"
                android:layout_height="129dp"
                android:layout_below="@+id/imageView12"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginStart="214dp"
                android:layout_marginLeft="214dp"
                android:layout_marginTop="12dp"
                android:layout_marginEnd="37dp"
                android:layout_marginRight="37dp" />


        </RelativeLayout>

        <TextView
            android:id="@+id/desc2"
            android:layout_width="match_parent"
            android:layout_height="5dp"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="15dp"
            android:layout_marginRight="30dp"
            android:layout_marginBottom="10dp"
            android:textColor="@color/black" />

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:srcCompat="@android:drawable/ic_menu_edit" />

        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:srcCompat="@android:drawable/ic_menu_edit" />


    </LinearLayout>
</RelativeLayout>
</androidx.cardview.widget.CardView>
